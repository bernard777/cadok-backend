{
  "name": "cadok-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:prod": "cross-env NODE_ENV=production MONGODB_URI=mongodb://localhost:27017/cadok nodemon server.js",
    "dev:test": "cross-env NODE_ENV=test MONGODB_URI=mongodb://localhost:27017/cadok_test nodemon server.js",
    "dev:dev": "cross-env NODE_ENV=development MONGODB_URI=mongodb://localhost:27017/cadok_dev nodemon server.js",
    "server:prod": "cross-env NODE_ENV=production MONGODB_URI=mongodb://localhost:27017/cadok node server.js",
    "server:test": "cross-env NODE_ENV=test MONGODB_URI=mongodb://localhost:27017/cadok_test node server.js",
    "server:dev": "cross-env NODE_ENV=development MONGODB_URI=mongodb://localhost:27017/cadok_dev node server.js",
    "db:status": "node scripts/check-db-status.js",
    "admin:create": "node scripts/create-admin-interactive.js",
    "categories:init": "node scripts/manage-categories.js init",
    "categories:reset": "node scripts/manage-categories.js reset",
    "categories:count": "node scripts/manage-categories.js count",
    "categories:list": "node scripts/manage-categories.js list",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:subscription": "cross-env NODE_ENV=test jest tests/subscription/",
    "test:services": "cross-env NODE_ENV=test jest tests/services/",
    "test:security": "cross-env NODE_ENV=test jest tests/security/",
    "test:webhooks": "cross-env NODE_ENV=test jest tests/webhooks/",
    "test:integration": "cross-env NODE_ENV=test jest tests/integration/",
    "test:all": "cross-env NODE_ENV=test jest --coverage --verbose",
    "test:critical": "cross-env NODE_ENV=test jest tests/services/ tests/security/ tests/webhooks/",
    "test:regression": "node ../test-anti-regression.js",
    "test:mock": "cross-env NODE_ENV=test jest --config=jest.config.mock.js",
    "test-data:create": "node scripts/create-comprehensive-test-data.js",
    "test-data:clean": "node scripts/create-clean-test-data.js",
    "super-admin:create": "node scripts/create-my-super-admin.js",
    "seed:production": "cross-env NODE_ENV=production node scripts/create-comprehensive-test-data.js",
    "seed:test": "cross-env NODE_ENV=test MONGODB_URI=mongodb://localhost:27017/cadok_test node scripts/create-comprehensive-test-data.js",
    "test-data:verify": "node scripts/verify-test-data.js",
    "test:unit": "cross-env NODE_ENV=test jest tests/services-unit-mock.test.js tests/basic-validation.test.js --config=jest.config.mock.js",
    "test:quick": "cross-env NODE_ENV=test jest tests/basic-validation.test.js tests/diagnosis.test.js --no-coverage",
    "test:anti-regression": "cross-env NODE_ENV=test jest tests/anti-regression.test.js --config=jest.config.mock.js",
    "test:images": "cross-env NODE_ENV=test jest tests/api-images-integration.test.js",
    "test:all-working": "npm run test:quick && npm run test:anti-regression",
    "prebuild": "npm test",
    "build": "echo 'Backend build completed successfully'",
    "build:prod": "npm run test && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@react-native-community/datetimepicker": "^8.4.4",
    "@trycourier/courier": "^6.5.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dompurify": "^3.2.6",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.4",
    "helmet": "^8.1.0",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.16.4",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.5",
    "pdfkit": "^0.17.1",
    "qrcode": "^1.5.4",
    "sanitize-html": "^2.17.0",
    "stripe": "^18.4.0",
    "twilio": "^5.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "axios": "^1.11.0",
    "babel-jest": "^29.7.0",
    "bcrypt": "^6.0.0",
    "cross-env": "^7.0.3",
    "jest": "^29.0.0",
    "mongodb-memory-server": "^9.5.0",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.4"
  }
}
